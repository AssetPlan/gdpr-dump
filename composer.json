{
    "name": "smile/gdpr-dump",
    "description": "A utility to create anonymized database dumps",
    "type": "library",
    "authors": [
        {
            "name": "Technical Office",
            "email": "dirtech@smile.fr"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.0",
        "doctrine/dbal": "^2.5",
        "fzaninotto/faker": "^1.8",
        "ifsnop/mysqldump-php": "^2.7",
        "justinrainbow/json-schema": "^5.2",
        "symfony/config": "^3.4",
        "symfony/console": "^3.4",
        "symfony/dependency-injection": "^3.4",
        "symfony/finder": "^3.4",
        "symfony/yaml": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "Smile\\GdprDump\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "bin": [
        "bin/compile",
        "bin/console"
    ]
}
